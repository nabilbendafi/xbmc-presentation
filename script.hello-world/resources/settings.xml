<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<settings>
  <category label="General">
    <setting id="debug" type="bool" label="debug" default="false"/>
    <setting id="timeout" type="slider" label="timeout" default="1000" range="1000,1000,10000"/>
    <setting id="username" type="text" label="username" default="John Doe"/>
    <setting id="cardnumber" type="number" label="cardnumber" default="007"/>
    <setting id="expirationdate" type="date" label="expiration date" default="1970-01-01"/>
    <setting id="time" type="time" label="time" default="00:00"/>
    <setting id="filepath" type="file" label="filepath" value="smb://somewhere"/>
    <setting id="mute" type="action" label="mute" action="Mute"/>
    <setting id="ipaddress" type="ipaddress" label="ip address" default="127.0.0.1" />
  </category>
</settings>
